{ 
    "manifest_version": 3, 
    "name": "Canvas Quiz Scraper", 
    "version": "1.0", 
    "permissions": ["scripting", "activeTab", "storage","downloads"], 

    "background": {
        "service_worker": "background.bundle.js", 
        "type": "module"
    },

    "host_permissions": [
        "https://generativelanguage.googleapis.com/",
        "https://*.canvas.edu/*" 
    ],
    "action": { 
        "default_popup": "MainMenu.html" 
    }, 
    "content_scripts": [
        { 
            "matches": ["https://*.canvas.edu/*"], 
            "js": ["popup.js"], "type": "module" 
        } 
    ], 
    "web_accessible_resources": [ 
        { 
            "resources": [ "classes/**/*.js"], 
            "matches": ["<all_urls>"] 
        } 
    ] 
}

