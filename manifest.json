{ 
    "manifest_version": 3, 
    "name": "Canvas Quiz Scraper", 
    "version": "1.0", 
    "permissions": ["scripting", "activeTab"], 
    "action": { "default_popup": "MainMenu.html" }, 
    "content_scripts": [ { "matches": ["<all_urls>"], 
    "js": ["popup.js"], "type": "module" } ], 
    "web_accessible_resources": [ 
        { "resources": [ 
            "classes/FormClasses/*.js", 
            "classes/QuestionClasses/*.js", 
            "classes/QuizClass/*.js",
            "classes/Controller/*.js"
        ], 
        "matches": ["<all_urls>"] } 
    ] 
}